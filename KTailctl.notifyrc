[Global]
IconName=ktailctl
Name=ktailctl
Comment=Tailscale control center for KDE plasma
DesktopEntry=ktailctl.desktop

[Context/status]
Name=Status
Comment=Status of Tailscale

[Context/peer]
Name=Peer
Comment=Status of a Tailscale peer

[Event/down]
Name=Tailscale offline
Comment=Tailscale is now offline
Contexts=status

[Event/up]
Name=Tailscale online
Comment=Tailscale is now online
Contexts=status